{
  "name": "clodds",
  "version": "1.6.4",
  "description": "Open-source AI trading terminal. Trade Polymarket, Kalshi, Betfair, Binance, Bybit, Hyperliquid, Solana DEXs from Telegram, Discord, Slack or web. Arbitrage detection, whale tracking, copy trading, perpetual futures up to 200x, DeFi swaps, MEV protection. Self-hosted, 119 skills, powered by Claude.",
  "main": "dist/index.js",
  "bin": {
    "clodds": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && node -e \"const{cpSync,readdirSync}=require('fs'),p=require('path');readdirSync('src/skills/bundled',{withFileTypes:true}).filter(d=>d.isDirectory()).forEach(d=>{const s=p.join('src/skills/bundled',d.name,'SKILL.md'),t=p.join('dist/skills/bundled',d.name,'SKILL.md');try{cpSync(s,t)}catch{}})\"",
    "start": "node dist/index.js",
    "gateway": "tsx src/gateway/index.ts",
    "worker": "tsx src/bin/worker.ts",
    "onboard": "tsx src/cli/onboard.ts",
    "typecheck": "tsc --noEmit",
    "test": "node --test --import tsx --import ./tests/helpers/test-setup.ts tests/**/*.test.ts",
    "ci": "npm run typecheck && npm run test && npm run build",
    "prepublishOnly": "npm run build",
    "publish:npm": "npm publish --registry https://registry.npmjs.org"
  },
  "keywords": [
    "trading",
    "prediction-markets",
    "polymarket",
    "kalshi",
    "betfair",
    "agentbets",
    "crypto",
    "defi",
    "futures",
    "binance",
    "solana",
    "ethereum",
    "arbitrage",
    "whale-tracking",
    "copy-trading",
    "mev-protection",
    "ai",
    "claude",
    "telegram-bot",
    "discord-bot",
    "trading-bot",
    "lending",
    "marginfi",
    "solend",
    "kamino"
  ],
  "author": "alsk1992",
  "homepage": "https://cloddsbot.com",
  "bugs": {
    "url": "https://github.com/alsk1992/CloddsBot/issues"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "license": "MIT",
